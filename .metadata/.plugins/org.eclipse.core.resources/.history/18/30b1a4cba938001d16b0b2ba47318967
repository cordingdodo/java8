package se02;

public class Car {
	String name; //멤버 필드 : 객체(인스턴스)가 가지는 특성을 저장
	String fuel;
	int cc;
	int speed; 
	//[접근제한자] [수정자](모디큐어) 데이터 타입 필드명;
	
	public Car () {} //생성자 함수는 접근제한자 안 넣음
	public Car (String name) { this.name = name;} //앞 개체속성 뒤 매개변수
	public Car (String name, String fuel) { this.name = name; this.fuel=fuel; } 
	public Car (String name, String fuel, int cc) { this(name, fuel, cc, 0); } 
	//디스함수 현재 객체의 생성자함수 뜻함 //하나 넣으면 1번 name 불러옴  // 두개 넣으면 2번째 호출 / 자기자신은 못 넣음
	
	public Car(String name, String fuel, int cc, speed){
		
	}
	
}
