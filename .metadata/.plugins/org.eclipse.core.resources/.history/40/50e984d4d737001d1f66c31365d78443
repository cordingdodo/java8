package src1;

public class Ex7 {

	public static void main(String[] args) {
		//
		int max = 0, min =9;
		int [] arry = { 1,5,3,8,2};
		
		for(int i=0; 1<arry.length; i++){
			if (arry[i]>max) {
				max = arry[i];
			}
		}
		
		System.out.println("max:" + max);
		System.out.println("min:" + min);

	}

}
