package se02;

public class Student {
	String name;
	int kor;
	int eng;
	int mat;
			
	public Student () {}
	public Student (String name) {this.name = name;}
	public Student (String name, int kor, int eng, int mat) {this(name, 0, 0, 0);}
	
	public Student (String name, int kor, int eng, int mat){
					this.name=name; this.kor=kor; this.eng=eng; int.mat=mat;
					
	}

}
