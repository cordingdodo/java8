package se02;

import java.util.Scanner;

public class Productex {

	public static void main(String[] args) {
		Scanner sc = new Scanner(system.in);
		Product pr1 = new Product ();
		pr1.setPid("a01");
		pr1.setPname("냉장고");
		pr1.setAmount(3);
		pr1.setPrice(80000);
		pr1.setImg(icetank01.jpg);
		
		pr1.printProduct();
		System.out.println(pr1.calcRank());
	}

}
