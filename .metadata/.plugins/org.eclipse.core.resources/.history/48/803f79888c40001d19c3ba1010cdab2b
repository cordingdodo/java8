package sec2;
public class SystemEx1 {
	public static void main(String[] args) {
		//보안 관리자 설정: 특정 조건(값이 5일때 종료)
		System.setSecurityManager(new SecurityManage(){
			@Override
			public void checkExit(int status){
				if(status != 5){
					throw new SecurityException();
				}
			}
			
		});
		for(int i=0;i<10;i++){
			System.out.println();
			System.exit(i);
		}

	}

}
