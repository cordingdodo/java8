package dodohw;

public class Account {
	String number; 
	String name;
	int money;
	int amount;
	int i;
	
	public Account () {}
	public Account (String number) {this.number=number;}
	public Account (String number, String name) {this.number=number; this.name=name;}
	public Account (String number, String name, int money) {this.number=number; this.name=name; this.money=money;}
	public Account (String number, String name, int money, int amount)
	{this.number=number; this.name=name; this.money=money; this.amount=amount;}
	public String getNumber() {
		return number;
	}
	public void setNumber(String number) {
		this.number = number;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getMoney() {
		return money;
	}
	public void setMoney(int money) {
		this.money = money;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;

	//*************************메서드//
	public void runDeposit(amount) {
		money+=amount
		System.out.println("입금되었습니다");
		
	}
	}
	

}
