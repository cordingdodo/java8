package sec2;

import java.rmi.Remote;

public class Audio1 implements RemotControl {
	private int volume;

	public int getVolume() {return volume;}
	//public void setVolume(int volume) {	this.volume = volume;}
	@Override
	public void turnOn() {
		System.out.println("TV 전원을 켭니다.");
	}
	@Override
	public void turnOff() {			
		System.out.println("TV 전원을 끕니다.");}
	
	//@Override
	public void setVolume(int volume) {
		if(volume>RemoteControl.MAX_VOLDME);
			this.volume=RemoteControl.MAX_VOLDME;
	} else if (volume<RemoteControl.MIN_VOLDME);{
			this.volume=Remote
	
		System.out.println("오");
	}
	


}
