package dodohw;

public class Account {
	String number; 
	String name;
	int money;
	int amount;
	int i;
	
	public Account () {}
	public Account (String number) {this.number=number;}
	public Account(String number, String name, int money, int amount, int i) {
		this.number = number;
		this.name = name;
		this.money = money;
		this.amount = amount;
		this.i = i;
	}
	public String getNumber() {
		return number;
	}
	public void setNumber(String number) {
		this.number = number;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getMoney() {
		return money;
	}
	public void setMoney(int money) {
		this.money = money;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	public int getI() {
		return i;
	}
	public void setI(int i) {
		this.i = i;
	}
	public String runDeposit() {
		money+=amount;
		System.out.println("입금되었습니다.");
	}


}
