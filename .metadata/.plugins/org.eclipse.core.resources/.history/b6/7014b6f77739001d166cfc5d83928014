package se02;

public class Product {
	private String pid;
	private String pname;
	private int amount;
	private int price;
	private String img;
	
	public Product () {}
	public Product (String pid) {this.pid=pid;}
	public Product(String pid, String pname, int amount, int price, String img) {
		this.pid = pid;
		this.pname = pname;
		this.amount = amount;
		this.price = price;
		this.img = img;
	}
	
}
