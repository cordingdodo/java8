package ch002;

public class Var03 {
	public static void main(String[] args) {
		//primitive type (기본 자료형): 하나의 데이터만 저장 가능한 구조
		boolean a=true;
		byte b=20, c=127; //-128~127 ->1byte (8bit) 2의 8승 256가지 오버플로우/언더플로우
		short d=6450; //2byte (16bit) = 2에 16승  65536가지 -4
		int e=65000; //4byte (32bit) =2에 31승 -1
		long f=650000; //	8byte (64bit) 
		float g=3.14f; //4byte (32bit)
		double h=3.14; //8bite
		//문자/문자열 
		char i='k';  //2byte (16bit) 참조형
				
				

		String j="dodo";
				
	
	

		
		
		
		
		
		
		
		
		
		
		
		
		
		
		[]

	}

}
