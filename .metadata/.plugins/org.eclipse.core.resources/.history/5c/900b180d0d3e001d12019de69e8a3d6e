package sec4;

public abstract class Student { //p.320
	Stirng schoolName;
	String bun;
	String name;
	int jum1;
	int jum2;
	int jum3;
	
	abstract int computeTot(int jum1, int jum2, int jum3); //추상화 : 선언만 함 //abstract 반환타입 메소드 이름 (메개변수) : 매개변수 갯수가 맞아야함
	abstract double computeAvg(int jum1, int jum2, int jum3);
	
}
