package sec1;
//throw는 예외 떠넘기기  
public class Throw1 { 
	public static void main(String[] args) {
		String name = null;
		nullPassing(name);
		
		int[] arr = new int[3];
		arrayPassing(arr);
	}
	static void nullPassing(String data) throws NullPointerException{
		System.out.println(data); //null값 출력된다는 게 에러임..ㅋㅋ
	}

}
