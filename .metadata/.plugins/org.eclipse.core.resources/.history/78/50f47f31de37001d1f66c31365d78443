package src1;

public class Ex8 {

	public static void main(String[] args) {
		int[] [] array = {{95, 86},
				{83,92,96},
				{78,83,93,87,88}};
		
		int count=0;
		for(int=i;i<array.length;i++){
			for(int=j; j<array[i].length;j++){
				sum+=array[i][j];		
			}
			count += array[i].length;
					
		}
		
		avg = (doulbe) sum / count;
		
		int sum = 0;
		double avg = 0.0;
		
		System.out.println();
		System.out.println();

	}

}
